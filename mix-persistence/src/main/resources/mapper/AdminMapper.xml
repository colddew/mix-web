<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.ustc.mix.persistence.mapper.AdminMapper">
	
	<resultMap id="BaseResultMap" type="edu.ustc.mix.persistence.entity.Admin">
		<id column="aid" property="aid" jdbcType="BIGINT" />
		<result column="name" property="name" jdbcType="VARCHAR" />
		<result column="password" property="password" jdbcType="VARCHAR" />
	</resultMap>
	
	<sql id="Base_Column_List">
		aid, name, password
	</sql>
	
	<select id="list" resultMap="BaseResultMap" parameterType="hashmap">
		select
		<include refid="Base_Column_List" />
		from sys_admin
	</select>
	
</mapper>